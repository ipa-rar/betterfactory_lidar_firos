<launch>

  <arg name="tf_node" default="tf" />
  <arg name="lidar_node" default="rplidar_ros" />

  <include file="$(find rplidar_ros)/launch/rplidar.launch" />
  <node name="$(arg tf_node)" pkg="tf" type="static_transform_publisher" args="0 0 0 0 0 0 1 map cloud 100">
  </node>
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find betterfactory_bringup)/config/visualization.rviz">
  </node>

</launch>